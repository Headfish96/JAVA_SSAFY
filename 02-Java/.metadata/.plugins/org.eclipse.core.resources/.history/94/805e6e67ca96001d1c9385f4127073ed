package com.ssafy.c_inheritance.person;

public class SpiderTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SpiderMan sman = new SpiderMan(); //SpiderMan 객체생성
		sman.isSpider = false;
		sman.name = "최하영";
		sman.eat();
		sman.jump();
		sman.isSpider = true;
		sman.fireWeb();
		
	}

}
