package com.ssafy.c_inheritance.person;

public class Person {
	
	String name;
	void eat() {
		System.out.println("냠냠");
	}
	
	void eat(String tool) {
		System.out.print(tool+"을 이용해서 ");
		//System.out.println("냠냠");
		this.eat(); //this를 통해 같은 클래스의 다른 메소드를 불러옴
	}
	
	void jump() {
		System.out.println("폴짝");
	}
}
