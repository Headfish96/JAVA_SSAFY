package com.ssafy.day01;

public class ArrayTest {
	static int N, M;
	public static void main(String[] args) {
		N = 3;
		M = 3;
		int[][] arr = {
				{1,2,3},
				{4,5,6},
				{7,8,9},
		};		
		exam01(arr); //행 우선 순회하기	
	}
	
	/*
	 * 1 2 3
	 * 4 5 6
	 * 7 8 9
	 * 
	 * 위와 같이 출력해보아라
	 */
	
	private static void exam01(int[][] arr) {
		//System.out.println(arr.length);
		for(int i = 0; i < N; i++) {
			for(int j = 0; j < M; j++) {
				System.out.println(arr[i][j] + "");
			}
			System.out.println();
		}
	}
	

}
